{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "ConneXGraphQlClient",
      "namespace": "DataIO.ConneX.API.GraphQl",
      "url": "http://localhost:5001/graphql",
      "dependencyInjection": true,
      "records": {
        "inputs": false,
        "entities": false
      },
      "transportProfiles": [
        {
          "default": "Http",
          "subscription": "WebSocket"
        }
      ]
    }
  }
}